import java.util.HashMap;

public class HashMapExample
{
	public static void main(String args[])
	{
		HashMap <Integer,String> hs = new HashMap <Integer,String>();
		hs.put(100,"anupam");
		hs.put(104,"gaurav");
		hs.put(102,"satyam");
		hs.put(103,"mousumi");
		hs.put(101,"amaani");
		
		System.out.println(hs);
	}
}
